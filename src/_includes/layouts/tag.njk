---
layout: layouts/base.njk
eleventyComputed:
  title: "Tag: {{ tag }}"
---

<main class="container">
  <article class="article-body">
      <header>
          <h1 class="tag">{{ tag }}</h1>
      </header>
      <section>
          <ol>
          {% set taglist = collections[ tag ] %}
          {% for post in taglist | reverse %}
            <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
          {% endfor %}
          </ol>
      </section>
  </article>
</main>